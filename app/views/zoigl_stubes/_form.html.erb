<%= simple_form_for(@zoigl_stube) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :stube_name %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :rbid %>
    <%= f.input :founded %>
    <%= f.input :score %>
    <%= f.input :lat %>
    <%= f.input :lon %>
    <%= f.input :web %>
    <%= f.input :hours %>
    <%= f.input :description %>
    <%= f.input :phone %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
