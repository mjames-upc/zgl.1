<%- model_class = ZoiglStube -%>
<div class="page-header">
  <h1><%=t '.title', :default => ZoiglStube %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:stube_name) %></dt>
      <dd><pre class="prettyprint"><%= @zoigl_stube.stube_name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:address) %></dt>
      <dd><pre class="prettyprint"><%= @zoigl_stube.address %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:city) %></dt>
      <dd><pre class="prettyprint"><%= @zoigl_stube.city %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:rbid) %></dt>
      <dd><pre class="prettyprint"><%= @zoigl_stube.rbid %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:founded) %></dt>
      <dd><pre class="prettyprint"><%= @zoigl_stube.founded %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:score) %></dt>
      <dd><pre class="prettyprint"><%= @zoigl_stube.score %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:lat) %></dt>
      <dd><pre class="prettyprint"><%= @zoigl_stube.lat %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:lon) %></dt>
      <dd><pre class="prettyprint"><%= @zoigl_stube.lon %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:web) %></dt>
      <dd><pre class="prettyprint"><%= @zoigl_stube.web %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:hours) %></dt>
      <dd><pre class="prettyprint"><%= @zoigl_stube.hours %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:description) %></dt>
      <dd><pre class="prettyprint"><%= @zoigl_stube.description %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:phone) %></dt>
      <dd><pre class="prettyprint"><%= @zoigl_stube.phone %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              zoigl_stubes_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_zoigl_stube_path(@zoigl_stube), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              zoigl_stube_path(@zoigl_stube),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

